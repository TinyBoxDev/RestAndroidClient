package it.enjinia.ztl.alert.net;

import java.util.HashMap;

public class ZtlAlertLoginPacket extends ZtlAlertPacket {
	
	public static final String USERNAME = "username";
	public static final String PASSWORD = "password";
	
	public ZtlAlertLoginPacket(String username, String password) {
		this.put(ZtlAlertLoginPacket.USERNAME, username);
		this.put(ZtlAlertLoginPacket.PASSWORD, password);
	}

	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return null;
	}

}
